{
  "listeners": {
    "*:18880": {
      "pass": "applications/n2t"
    }
  },
  "applications": {
    "n2t": {
      "type": "python 3.12",
      "working_directory": "/n2t",
      "path": [
        "/n2t"
      ],
      "module": "n2t.app",
      "callable": "app",
      "environment": {
        "N2T_SETTINGS":"/n2t/dev-config.env",
        "PYTHONPATH": "/n2t"
      }
    }
  },
  "routes": {
    "n2t": [
      {
        "match": {
          "uri": [
            "/*"
          ]
        },
        "action": {
          "pass": "applications/n2t"
        }
      }
    ]
  },
  "access_log": {
    "path": "/dev/stdout",
    "format": {
      "log_source": "n2t-access-log",
      "remote_addr": "$remote_addr",
      "header_x_forwarded_for": "$header_x_forwarded_for",
      "time_local": "$time_local",
      "request_line": "$request_line",
      "status": "$status",
      "body_bytes_sent": "$body_bytes_sent",
      "header_referer": "$header_referer",
      "$header_user_agent": "$header_user_agent"
    }
  }
}

